<?xml version="1.0" encoding="UTF-8"?>
<model>
	<params group="JL_EXT_EXTENDED_PREFERENCES" addpath="/administrator/components/com_joomleague/elements">
	
		<param 	name="JL_EXT_PROJECTTEAM_LESSONS" type="list" 
				label="JL_EXT_PROJECTTEAM_LESSONS"
				description="Theoretical lessons day">
			<option value="monday">Monday</option>
			<option value="tuesday">Tuesday</option>
			<option value="wednesday">Wednesday</option>
			<option value="thursday">Thursday</option>
			<option value="friday">Friday</option>
			<option value="saturday">Saturday</option>
			<option value="sunday">Sunday</option>
		</param>
		
		<param 	name="JL_EXT_PROJECTTEAM_TEACHING_STARTTIME" type="calendar"
				label="JL_EXT_PROJECTTEAM_TEACHING_STARTTIME" 
				description="Teaching start time">
		</param>
		
		<param 	name="JL_EXT_PROJECTTEAM_TEACHING_ENDTIME" type="calendar"
				label="JL_EXT_PROJECTTEAM_TEACHING_ENDTIME" 
				description="Teaching end time">
		</param>
		
		<param 	name="JL_EXT_PROJECTTEAM_TEACHING_PLACE" type="text"
				label="JL_EXT_PROJECTTEAM_TEACHING_PLACE" 
				description="Teaching place">
		</param>
		
		<param 	name="JL_EXT_PROJECTTEAM_INFOTEXT" type="textarea" rows="4" cols="50"
				label="JL_EXT_PROJECTTEAM_INFOTEXT" 
				description="Infotext">
		</param>
		
	</params>
</model>