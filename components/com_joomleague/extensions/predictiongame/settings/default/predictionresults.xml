<?xml version="1.0"?>
<templateconfig type="JL_template" version="1.0.0">
	<name><![CDATA[JL_FES_PREDICTIONRESULTS_NAME]]></name>
	<description><![CDATA[JL_FES_PREDICTIONRESULTS_DESCR]]></description>

	<params	group= "JL_FES_PARAMS_GROUP_OPTIONS"
			description="JL_FES_PARAMS_DESCR_OPTIONS">

		<param	name="show_matchday_pagenav" type="list" default="1"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_PAGE_NAV"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_PAGE_NAV">
			<option value="0">JL_FES_PREDICTIONRESULTS_PARAM_OPTION0_SHOW_PAGE_NAV</option>
			<option value="1">JL_FES_PREDICTIONRESULTS_PARAM_OPTION1_SHOW_PAGE_NAV</option>
		</param>

		<param	name="show_all_user" type="radio" default="0"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_ALL_USERS"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_ALL_USERS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="limit" type="text" size="3" maxlength="2" default="10"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_LIMIT_USERS"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_LIMIT_USERS" />

		<param	name="link_name_to" type="radio" default="0"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_LINK_USERS_TO"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_LINK_USERS_TO">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="show_user_icon" type="radio" default="0"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_USERS_ICON"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_USERS_ICON">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="show_average_points" type="radio" default="1"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_AVERAGE_POINTS"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_AVERAGE_POINTS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="show_points" type="radio" default="1"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_POINTS"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_POINTS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="show_logo_small_overview" type="radio" default="1"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_CLUB_ICON"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_CLUB_ICON">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_FES_RESULTS_PARAM_OPTION1_LOGO_SMALL</option>
			<option value="2">JL_FES_RESULTS_PARAM_OPTION2_LOGO_SMALL</option>
		</param>

		<param	name="show_help" type="radio" default="0"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_HELP"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_HELP">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param	name="show_scoring" type="radio" default="1"
				label="JL_FES_PREDICTIONRESULTS_PARAM_LABEL_SHOW_SCORING"
				description="JL_FES_PREDICTIONRESULTS_PARAM_DESCR_SHOW_SCORING">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

	</params>

</templateconfig>
