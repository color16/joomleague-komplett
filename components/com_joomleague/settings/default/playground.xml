			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param name="show_maps" type="radio" default="1"
			label="JL_FES_PLAYGROUND_PARAM_LABEL_SHOW_MAPS" description="JL_FES_PLAYGROUND_PARAM_DESCR_SHOW_MAPS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
