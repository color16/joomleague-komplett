			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param name="show_teams" type="radio" default="1"
			label="JL_FES_PLAYGROUND_PARAM_LABEL_SHOW_TEAMS" description="JL_FES_PLAYGROUND_PARAM_DESCR_SHOW_TEAMS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param name="show_matches" type="radio" default="1"
			label="JL_FES_PLAYGROUND_PARAM_LABEL_SHOW_MATCHES" description="JL_FES_PLAYGROUND_PARAM_DESCR_SHOW_MATCHES">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

	</params>

	<params group="JL_FES_PARAMS_GROUP_OPTIONS" description="JL_FES_PARAMS_DESCR_OPTIONS">

		<param name="show_route" type="radio" default="1"
