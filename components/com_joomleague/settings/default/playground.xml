		<param name="show_logo" type="radio" default="1"
			label="JL_FES_PLAYGROUND_PARAM_LABEL_SHOW_LOGO" description="JL_FES_PLAYGROUND_PARAM_DESCR_SHOW_LOGO">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
                
        <param 	name="show_shortname" type="radio" default="0" 
