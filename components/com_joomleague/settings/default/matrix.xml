<?xml version="1.0"?>
<templateconfig type="JL_template" version="1.5.0">
    <name>JL_FES_MATRIX_NAME</name>
    <description>JL_FES_MATRIX_DESCR</description>

    <params group="JL_FES_PARAMS_GROUP_PAGE_ELEMENTS" 
			description="JL_FES_PARAMS_DESCR_PAGE_ELEMENTS">
  
		<param 	name="show_sectionheader" type="radio" default="1" 
				label="JL_FES_MATRIX_PARAM_LABEL_SHOW_HEADER" 
				description="JL_FES_MATRIX_PARAM_DESCR_SHOW_HEADER">
              <option value="0">JL_GLOBAL_NO</option>
              <option value="1">JL_GLOBAL_YES</option>
        </param>	

		<param 	name="show_matrix" type="radio" default="1" 
				label="JL_FES_MATRIX_PARAM_LABEL_SHOW_MATRIX" 
				description="JL_FES_MATRIX_PARAM_DESCR_SHOW_MATRIX">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>v		  
		  
    </params>

    <params group="JL_FES_PARAMS_GROUP_OPTIONS" 
			description="JL_FES_PARAMS_DESCR_OPTIONS">
			
        <param 	name="crosstable_icons_horizontal" type="radio" default="0" 
				label="JL_FES_MATRIX_PARAM_LABEL_SHOW_HORZ_ICONS" 
				description="JL_FES_MATRIX_PARAM_DESCR_SHOW_HORZ_ICONS">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        <param 	name="crosstable_icons_vertical" type="radio" default="0" 
				label="JL_FES_MATRIX_PARAM_LABEL_SHOW_VERT_ICONS" 
				description="JL_FES_MATRIX_PARAM_DESCR_SHOW_VERT_ICONS">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        <param 	name="link_teams" type="radio" default="1" 
				label="JL_FES_MATRIX_PARAM_LABEL_LINK_TEAMNAMES" 
				description="JL_FES_MATRIX_PARAM_DESCR_LINK_TEAMNAMES">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        <param 	name="force_link_report" type="radio" default="0" 
				label="JL_FES_MATRIX_PARAM_LABEL_FORCE_LINK_REPORT" 
				description="JL_FES_MATRIX_PARAM_DESCR_FORCE_LINK_REPORT">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        <param 	name="which_link" type="list" default="1" 
				label="JL_FES_MATRIX_PARAM_LABEL_WHICH_LINK" 
				description="JL_FES_MATRIX_PARAM_DESCR_WHICH_LINK">
            <option value="0">JL_FES_MATRIX_PARAM_OPTION0_WHICH_LINK</option>
            <option value="1">JL_FES_MATRIX_PARAM_OPTION1_WHICH_LINK</option>
            <option value="2">JL_FES_MATRIX_PARAM_OPTION2_WHICH_LINK</option>
        </param>
        <param 	name="highlight_fav_team" type="list" default="0" 
				label="JL_FES_MATRIX_PARAM_LABEL_HIGHLIGHT_FAVTEAM" 
				description="JL_FES_MATRIX_PARAM_DESCR_HIGHLIGHT_FAVTEAM">
            <option value="0">JL_FES_MATRIX_PARAM_OPTION0_HIGHLIGHT_FAVTEAM</option>
            <option value="1">JL_FES_MATRIX_PARAM_OPTION1_HIGHLIGHT_FAVTEAM</option>
            <option value="2">JL_FES_MATRIX_PARAM_OPTION2_HIGHLIGHT_FAVTEAM</option>
        </param>
        <param 	name="show_help" type="radio" default="0" 
				label="JL_FES_MATRIX_PARAM_LABEL_SHOW_HELP" 
				description="JL_FES_MATRIX_PARAM_DESCR_SHOW_HELP">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
    </params>
	
	<params group="JL_FES_PARAMS_GROUP_ADVANCED_OPTIONS" 
			description="JL_FES_PARAMS_DESCR_ADVANCED_OPTIONS">	
		
		<param 	name="style_class1" type="text" default="sectiontableentry1" 
				label="JL_FES_PARAM_LABEL_STYLE_ODD" 
				description="JL_FES_PARAM_DESCR_STYLE_ODD">
		</param>
		
		<param 	name="style_class2" type="text" default="sectiontableentry2" 
				label="JL_FES_PARAM_LABEL_STYLE_EVEN" 
				description="JL_FES_PARAM_DESCR_STYLE_EVEN">
		</param>
		
	</params>		
</templateconfig>