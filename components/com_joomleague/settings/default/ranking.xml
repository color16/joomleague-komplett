			<option value="middle_name">JL_FES_RANKING_PARAM_OPTION1_TYPE_NAME</option>
			<option value="name">JL_FES_RANKING_PARAM_OPTION2_TYPE_NAME</option>
		</param>

		<param type="spacer">
		</param>
		
		<param 	name="show_icons" type="list" default="1" 
