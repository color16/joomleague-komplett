			<option value="1">JL_GLOBAL_YES</option>
		</param>

	    <param 	name="show_level1_table" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SUB1" 
				description="JL_FES_RANKING_PARAM_DESCR_SUB1">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

	    <param 	name="show_level2_table" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SUB2" 
				description="JL_FES_RANKING_PARAM_DESCR_SUB2">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

	</params>

	<params group="JL_FES_PARAMS_GROUP_TEAMOPTIONS" 
			description="JL_FES_PARAMS_DESCR_TEAMOPTIONS">

		<param 	name="show_logo_small_table" type="list" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_CLUB_LOGO" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_CLUB_LOGO">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_FES_RANKING_PARAM_OPTION1_SHOW_CLUB_LOGO</option>
			<option value="2">JL_FES_RANKING_PARAM_OPTION2_SHOW_CLUB_LOGO</option>
			<option value="3">JL_FES_RANKING_PARAM_OPTION3_SHOW_CLUB_LOGO</option>
      <option value="4">JL_FES_RANKING_PARAM_OPTION4_SHOW_CLUB_LOGO</option>
		</param>

		<param 	name="names" type="list" default="name" 
				label="JL_FES_RANKING_PARAM_LABEL_TYPE_NAME" 
