			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
		<param 	name="show_teamstats_link" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_TEAMSTATS_LINK" 
				description="JL_FES_PARAM_DESCR_TEAMSTATS_LINK">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
		<param 	name="show_plan_link" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_TEAMPLAN_LINK" 
				description="JL_FES_PARAM_DESCR_TEAMPLAN_LINK">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_clubplan_link" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_CLUBPLAN_LINK" 
				description="JL_FES_PARAM_DESCR_CLUBPLAN_LINK">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
		<param 	name="show_curve_link" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_CURVE_LINK" 
				description="JL_FES_PARAM_DESCR_CURVE_LINK">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

