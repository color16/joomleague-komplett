  <name><![CDATA[JL_FES_RANKING_NAME]]></name>
  <description><![CDATA[JL_FES_RANKING_DESCR]]></description>

	<params group="JL_FES_PARAMS_GROUP_PAGE_ELEMENTS"
			description="JL_FES_PARAMS_DESCR_PAGE_ELEMENTS">

		<param	name="show_sectionheader" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_SHOW_SECTIONHEADER" 
				description="JL_FES_PARAM_DESCR_SHOW_SECTIONHEADER">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_rankingnav" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_RANKINGNAV"
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_RANKINGNAV">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_ranking" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_RANKING" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_RANKING">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_colorlegend" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_LEGEND" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_LEGEND">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_explanation" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_EXPL" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_EXPL">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
		<param 	name="show_pagnav" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_PAGNAV" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_PAGNAV">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
        <param 	name="show_help" type="radio" default="0" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_HELP" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_HELP">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        
        <param 	name="show_maps" type="radio" default="1" 
				label="JL_FES_RANKING_MAP_PARAM_LABEL_SHOW_HELP" 
				description="JL_FES_RANKING_MAP_PARAM_DESCR_SHOW_HELP">
            <option value="0">JL_GLOBAL_NO</option>
            <option value="1">JL_GLOBAL_YES</option>
        </param>
        
	</params>

	<params group="JL_FES_PARAMS_GROUP_OPTIONS" 
			description="JL_FES_PARAMS_DESCR_OPTIONS">

		<param 	name="column_sorting" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_COLUMN_SORT" 
				description="JL_FES_RANKING_PARAM_DESCR_COLUMN_SORT">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
		
		<param 	name="ranking_order" type="textarea" default="JL_POINTS, JL_PLAYEDASC, JL_DIFF, JL_FOR" 
				label="JL_FES_RANKING_PARAM_LABEL_RANKING_CRITERIA" 
				description="JL_FES_RANKING_PARAM_DESCR_RANKING_CRITERIA" 
				rows="4" cols="70">
		</param>

		<param 	name="ordered_columns" type="textarea" default="JL_PLAYED, JL_WINS, JL_TIES, JL_LOSSES, JL_POINTS, JL_SCOREFOR, JL_SCOREAGAINST, JL_DIFF" 
				label="JL_FES_RANKING_PARAM_LABEL_DISPLAY_COLUMNS" 
				description="JL_FES_RANKING_PARAM_DESCR_DISPLAY_COLUMNS" 
				rows="4" cols="70">
		</param>
		
		<param 	name="ordered_columns_names" type="textarea" default="Pl, W, D, L, Pts, SF, SA, +/-" 
				label="JL_FES_RANKING_PARAM_LABEL_COLUMNS_NAMES" 
				description="JL_FES_RANKING_PARAM_DESCR_COLUMNS_NAMES" 
				rows="4" cols="70">
		</param>		
<!-- is this still used?
		<param	name="sort_table" type="list" default="0" 
				label="Base the table on matchdays or dates?"
				description="If the table is calculated based on the dates of the matches, early results from later matchdays are included.">
			<option value="0">based on matchdays</option>
			<option value="1">based on dates</option>
		</param>
-->
		<param	name="alternative_legs" type="text" default="" 
				label="JL_FES_RANKING_PARAM_LABEL_ALT_LEG"
				description="JL_FES_RANKING_PARAM_DESCR_ALT_LEG">
		</param>
		
		<param 	name="last_ranking" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_PREV_RANK" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_PREV_RANK">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
				
		<param	name="show_wdl_teamplan_link" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_WDL_TEAMPLAN_LINK"
				description="JL_FES_RANKING_PARAM_DESCR_WDL_TEAMPLAN_LINK">				
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param name="nb_previous" type="text" default="5"
			label="JL_FES_RANKING_PARAM_LABEL_NB_PREVIOUSX" description="JL_FES_RANKING_PARAM_DESCR_NB_PREVIOUSX">
		</param>	
		
		<param 	name="show_manipulations" type="radio" default="1" 
				label="JL_FES_RANKING_PARAM_LABEL_SHOW_MANIPULATIONS" 
				description="JL_FES_RANKING_PARAM_DESCR_SHOW_MANIPULATIONS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>		

		<param 	name="colors" type="textarea" default="1,1,#bdd8be,Champions;2,2,#cee0ce,Champions League;3,3,#dce7dc,Qualification Champions League;4,5,#f7f6d0,Europa League;6,6,#fcfbed,UI Cup;16,18,#f6dfdb,Relegation;" 
				label="JL_FES_RANKING_PARAM_LABEL_TABLE_COLORS" 
				description="JL_FES_RANKING_PARAM_DESCR_TABLE_COLORS" 
				rows="4" cols="70">
		</param>
	</params>

	<params group="JL_FES_PARAMS_GROUP_DIV_OPTIONS" 
			description="JL_FES_PARAMS_DESCR_DIV_OPTIONS">

		<param 	name="default_division_view" type="list" default="0" 
				label="JL_FES_RANKING_PARAM_LABEL_VIEW" 
				description="JL_FES_RANKING_PARAM_DESCR_VIEW">
			<option value="0">JL_FES_RANKING_PARAM_OPTION0_VIEW</option>
			<option value="1">JL_FES_RANKING_PARAM_OPTION1_VIEW</option>
			<option value="2">JL_FES_RANKING_PARAM_OPTION2_VIEW</option>
