		<param 	name="team_picture_height" type="text" default="150"
				label="JL_FES_PARAM_LABEL_TEAM_PICTURE_HEIGHT" 
				description="JL_FES_PARAM_DESCR_TEAM_PICTURE_HEIGHT">
