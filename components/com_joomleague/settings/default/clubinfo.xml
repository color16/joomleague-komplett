				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_SEASON">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_club_shirt" type="radio" default="0" 
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_SHIRT" 
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_SHIRT">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

