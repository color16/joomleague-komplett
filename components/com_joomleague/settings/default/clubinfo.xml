  <description><![CDATA[JL_FES_CLUBINFO_DESCR]]></description>

	<params group="JL_FES_PARAMS_GROUP_PAGE_ELEMENTS" 
			description="JL_FES_PARAMS_DESCR_PAGE_ELEMENTS">

		<param 	name="show_sectionheader" type="radio" default="1" 
				label="JL_FES_PARAM_LABEL_SHOW_SECTIONHEADER" 
				description="JL_FES_PARAM_DESCR_SHOW_SECTIONHEADER">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

    <param 	name="show_tabs" type="radio" default="0"
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_TABS"
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_TABS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_SLIDER</option>
      <option value="2">JL_GLOBAL_TABS</option>
		</param>
    
		<param 	name="show_club_info" type="radio" default="1"
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_CLUBINFO"
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_CLUBINFO">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_extended" type="radio" default="1" 
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_EXTENDED"
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_EXTENDED">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>			
		
		<param 	name="show_maps" type="radio" default="1" 
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_MAPS" 
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_MAPS">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="show_teams_of_club" type="radio" default="1" 
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_TEAMS_CLUB" 
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_TEAMS_CLUB">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

    <param 	name="show_club_rssfeed" type="radio" default="1" 
				label="JL_FES_CLUBS_PARAM_LABEL_SHOW_CLUB_RSSFEED" 
				description="JL_FES_CLUBS_PARAM_DESCR_SHOW_CLUB_RSSFEED">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>
    
</params>

	<params group="JL_FES_PARAMS_GROUP_OPTIONS" 
			description="JL_FES_PARAMS_DESCR_OPTIONS">

		<param 	name="show_club_logo" type="radio" default="1" 
				label="JL_FES_CLUBINFO_PARAM_LABEL_SHOW_CLUB_LOGO" 
				description="JL_FES_CLUBINFO_PARAM_DESCR_SHOW_CLUB_LOGO">
			<option value="0">JL_GLOBAL_NO</option>
			<option value="1">JL_GLOBAL_YES</option>
		</param>

		<param 	name="team_picture_width" type="text" default="150"
				label="JL_FES_PARAM_LABEL_TEAM_PICTURE_WIDTH" 
				description="JL_FES_PARAM_DESCR_TEAM_PICTURE_WIDTH">
